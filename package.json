{
  "name": "gikopoi2",
  "version": "0.0.1",
  "description": "gikopoi2",
  "main": "build/index.js",
  "engines": {
    "node": "14.17.0"
  },
  "license": "Beerware",
  "scripts": {
    "serve-fe": "vue-cli-service serve",
    "serve-be": "ts-node-dev --respawn src/backend/index.ts",
    "build": "vue-cli-service build && npx tsc",
    "start": "node backend-build/backend/index.js",
    "dev-remote-persistor": "cross-env PERSISTOR_URL=http://localhost:8086 PERSISTOR_SECRET=hello ts-node-dev --respawn index.ts",
    "start-persistor": "cross-env PERSISTOR_SECRET=hello node state-persistor.js",
    "deploy-prod": "git push origin master:CI-PROD",
    "deploy-test": "git push origin HEAD:CI-TEST -f"
  },
  "dependencies": {
    "chess.js": "^0.12.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.16.2",
    "express-sslify": "^1.2.0",
    "got": "^11.8.1",
    "janus-videoroom-client": "^4.1.10",
    "lodash": "^4.17.20",
    "loglevel": "^1.7.1",
    "socket.io": "^4.0.2",
    "socket.io-client": "^4.1.3",
    "tripcode": "file:./src/backend/tripcode",
    "uuid": "^8.3.2",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/chess.js": "^0.11.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/got": "^9.6.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^16.3.1",
    "@types/uuid": "^8.3.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "nodemon": "^2.0.6",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "~4.1.5"
  }
}
